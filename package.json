{
  "name": "vscode-browserview",
  "description": "A real browser view inside VS Code",
  "publisher": "auchenberg",
  "engines": {
    "vscode": "^1.30.0"
  },
  "license": "MIT",
  "version": "0.1.0",
  "main": "./build/ext-src/extension.js",
  "activationEvents": [
    "onView:nodeDependencies",
    "onCommand:browserview.showInstance"
  ],
  "configuration": {
    "properties": {
      "browserView.startUrl": {
        "default": "https://code.visualstudio.com",
        "description": "The default start url for new BrowserView instances",
        "type": "string"
      }
    },
    "title": "BrowserView",
    "type": "object"
  },
  "contributes": {
    "commands": [
      {
        "category": "Browser",
        "command": "browserview.showInstance",
        "title": "Show Browser View"
      }
    ],
    "views": {
      "browser-view": [
        {
          "id": "nodeDependencies",
          "name": "Targets"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "resources/icon.svg",
          "id": "browser-view",
          "title": "Browser View"
        }
      ]
    }
  },
  "dependencies": {
    "event-emitter-enhancer": "^1.0.57",
    "eventemitter2": "^5.0.1",
    "get-chrome": "0.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "lodash": "^4.17.11",
    "puppeteer-core": "^1.11.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-loading-bar": "^0.0.7",
    "react-scripts-ts": "^3.1.0",
    "supports-color": "^6.0.0",
    "which-chrome": "^0.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.119",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3",
    "vscode": "^1.1.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auchenberg/vscode-browserview.git"
  },
  "scripts": {
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "eject": "react-scripts-ts eject",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json"
  }
}
